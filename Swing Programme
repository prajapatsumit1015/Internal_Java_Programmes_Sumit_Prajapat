
import javax.swing.*;

public class Simple {

    JFrame f;

    Simple() {
        f = new JFrame();

        JButton b1 = new JButton("Click");
        b1.setBounds(130, 100, 100, 40);

        f.add(b1);

        f.setSize(400, 500);
        f.setLayout(null);
        f.setVisible(true);
    }

    public static void main(String[] args) {
        new Simple();
    }
}
